server:
  port: 8000

spring:
 datasource:
  driver-class-name: com.mysql.cj.jdbc.Driver
  url: jdbc:mysql://localhost:3306/estate?serverTimezone=Asia/Seoul
  username: root
  password: root
  
 jpa:
  open-in-view: true
  hibernate:
   ddl-auto: update
   naming:
    physical-strategy: org.hibernate.boot.model.naming.PhysicalNamingStrategyStandardlmpl
  show-sql: true
  properties:
   hibernate.format_sql: false
