/*
 * package com.example.estate.service;
 * 
 * import java.util.List;
 * 
 * import org.springframework.beans.factory.annotation.Autowired; import
 * org.springframework.stereotype.Service; import
 * com.example.estate.entity.User; import
 * com.example.estate.repository.UserRepository;
 * 
 * import jakarta.transaction.Transactional; import lombok.AllArgsConstructor;
 * 
 * @AllArgsConstructor
 * 
 * @Service public class UserService {
 * 
 * @Autowired private UserRepository userRepository;
 * 
 * @Transactional public List<User> getUserList() { return
 * userRepository.findAll(); } }
 */